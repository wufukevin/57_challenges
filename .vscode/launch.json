      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "OS-COMMENT4": "If you have changed target frameworks, make sure to update the program path.",
      "program": "${workspaceFolder}/zoe-3-11/bin/Debug/net5.0/zoe-3-11.dll",
      "args": [],
      "cwd": "${workspaceFolder}/zoe-3-11",
